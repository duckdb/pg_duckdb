BEGIN;
    INSERT INTO t2 VALUES (1), (2), (3);
ERROR:  DuckDB queries cannot run inside a transaction block
END;
BEGIN;
    CREATE TEMP TABLE t3(a int);
ERROR:  DuckDB queries cannot run inside a transaction block
END;
BEGIN;
    DROP TABLE t2;
ERROR:  DuckDB queries cannot run inside a transaction block
END;
-- But plain postgres DDL and queries should work fine
BEGIN;
    CREATE TEMP TABLE t4(a int) USING heap;
